{
    "rewrites": [
        {
            "source": "/**",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/**",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                }
            ]
        }
    ],
    "public": "build",
    "cleanUrls": true,
    "single": true
}